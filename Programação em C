    float div=num1/num2;
    float sqrt1= sqrt(num1);
    float sqrt2= sqrt(num2);
    
switch(operador){
    case 1:
    printf("\nA raiz quadrada dos números é, respectivamente: ****%0.1f e %0.1f****",sqrt1,sqrt2);
